{
    "name": "decodelabs/cipher",
    "description": "Tools and systems to interact with JWTs",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3",
        "decodelabs/coercion": "^0.3",
        "decodelabs/glitch-support": "^0.5",
        "decodelabs/tightrope": "^0.2.0",

        "firebase/php-jwt": "^6.9"
    },
    "require-dev": {
        "decodelabs/dovetail": "^0.2.0",
        "decodelabs/harvest": "^0.5",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/dovetail": "<0.2",
        "decodelabs/harvest": "<0.5"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Cipher\\": "src/Cipher/"
        },
        "classmap": [
            "src/Dovetail/",
            "src/Harvest/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
